<?xml version="1.0" encoding="UTF-8"?>
<!-- Unpack the *-ctl-scripts.zip archive in the TE_BASE/scripts directory -->
<!-- Add this entry to TE_BASE/config.xml (//organization/standard) -->
<organization>
  <name>OGC</name>
  <standard>
    <name>Catalogue Service - Web (CSW)</name>
    <version>
      <name>${spec-version}</name>
      <suite>
        <namespace-uri>http://www.opengis.net/cite/cat30</namespace-uri>
        <prefix>tns</prefix>
        <local-name>ets-cat30-${version}</local-name>
        <title>Test suite ets-cat30</title>
        <description>Assesses conformance with respect to  
        the OGC Catalogue Services 3.0 Specification .</description>
        <BasicConformanceClasses>
          <conformanceClass>Basic-Catalogue</conformanceClass>
        </BasicConformanceClasses>
      </suite>
      <revision>
        <name>${version}</name>
        <status>Final</status>
        <sources>
          <source>cat30/${spec-version}/ctl</source>
        </sources>
        <webdir>cat30/${spec-version}/site</webdir>
      </revision>
    </version>
  </standard>
</organization>
